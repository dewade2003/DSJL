<Window x:Class="DSJL.Export.ChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ChartWindow" ShowInTaskbar="False" WindowStyle="None" Loaded="Window_Loaded">
    <Grid Name="grid" Height="260" Width="750">
        <Grid Name="progressGrid" Width="750" Height="245"/>
        <Grid Name="maxColumnGrid" Width="750" Height="245"/>
        <Grid Name="maxLineGrid" Width="750" Height="245"/>
        <Grid Name="oddAvgGrid" Width="350" Height="245" HorizontalAlignment="Left"/>
        <Grid Name="evenAvgGrid" Width="350" Height="245" HorizontalAlignment="Left"/>
    </Grid>
</Window>
